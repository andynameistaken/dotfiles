#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2021-09-13T03:15:13.448646

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/andy/Library/Application Support/JetBrains/Toolbox/apps/DataSpell/ch-0/213.2094.12/JetBrains DS 2021.3.app/Contents/MacOS/dataspell" $wait --args "${ideargs[@]}"