#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2021-09-13T03:15:13.446393

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/andy/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/212.5284.17/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"